{
  "name": "Input Cursor Focus",
  "version": "0.1.2",
  "manifest_version": 2,
  "description": "Focuses cursor in first text input if none is focused.",
  "icons": {
    "128" : "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js" : ["inj.js"]
    }
  ],
  "background": {
    "scripts": ["bg.js"]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}